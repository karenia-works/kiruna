.userinfo{
  float : left
  width : 80%
}
.userbtndiv{
  float : right
  width : 20%
}
.func_part {
  lost-column: 1 / 5
  position: fixed
  right: var(--theme-pad-05)
  top: var(--theme-pad-10)

  .line {
    width = 100%
  }

  .func_t {
    color: var(--palette-base05)
    margin-top: 5px
  }

  .func_h {
    color: var(--palette-base03)
    margin-top: 5px
  }
}
.left_part{
  lost-column: 4 / 5
  display: flex
  flex-direction: column
}
